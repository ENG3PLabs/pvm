{
    "name": "hjbdev/pvm",
    "description": "PHP Version Manager for Windows CLI",
    "keywords": ["phpvm", "pvm", "php", "version", "manager"],
    "homepage": "https://pvm.hjb.dev",
    "type": "library",
    "license": "MIT",
    "version": "0.2.0",
    "support": {
        "issues": "https://github.com/hjbdev/pvm/issues",
        "source": "https://github.com/hjbdev/pvm"
    },
    "authors": [
        {
            "name": "Harry Bayliss",
            "email": "harry@hjb.dev"
        }
    ],
    "require": {
        "php": "^7.0",
        "tightenco/collect": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["pvm"]
}
